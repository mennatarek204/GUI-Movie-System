      <TextField fx:id="txtMovieName" layoutX="211.0" layoutY="345.0" />
      <TextField fx:id="txtCode" layoutX="242.0" layoutY="518.0" />
      <TextField fx:id="txtDate" layoutX="461.0" layoutY="345.0" />
