<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="971.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pkg12thproject.AddMovie">
